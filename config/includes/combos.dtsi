
/ {
    combos {
        compatible = "zmk,combos";

        tab {
            timeout-ms = <100>;
            bindings = <&kp TAB>;
            key-positions = <10 11>;
            layers = <0>;
        };

        shift_tab{
            timeout-ms = <100>;
            bindings = <&kp LS(TAB)>;
            key-positions = <20 21>;
            layers = <0>;
        };

        bootloader {
            bindings = <&bootloader>;
            key-positions = <0 4 20 24>;
        };

        kp_double_quote {
            bindings = <&kp DOUBLE_QUOTES>;
            key-positions = <18 19>;
            layers = <4>;
        };

        kp_underscore {
            bindings = <&kp UNDERSCORE>;
            key-positions = <8 9>;
            layers = <4>;
        };
    };
};
